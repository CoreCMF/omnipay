{
    "name": "corecmf/omnipay",
    "type": "Library",
    "description": "corecmf omnipay package",
    "keywords": [
        "CoreCMF",
        "omnipay",
        "alipay",
        "wechat"
    ],
    "homepage": "https://github.com/CoreCMF/omnipay",
    "license": "MIT",
    "authors": [
        {
            "name": "bigrocs",
            "email": "bigrocs@qq.com",
            "homepage": "http://www.bigrocs.vip",
            "role": "Developer"
        }
    ],
    "require": {
        "corecmf/admin": "^2.3",
        "corecmf/socialite": "^2.3",
        "php-http/guzzle6-adapter": "^1.1",
        "laravel-omnipay/laravel-omnipay": "^3.0",
        "laravel-omnipay/omnipay-unionpay": "^3.0",
        "laravel-omnipay/omnipay-wechatpay": "^3.0",
        "laravel-omnipay/omnipay-alipay": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "CoreCMF\\Omnipay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreCMF\\Omnipay\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
